
 <web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

   <servlet>
      <servlet-name>controller.GeneroController</servlet-name>
      <servlet-class>controller.GeneroController</servlet-class>
    </servlet>
 
    <servlet-mapping>
      <servlet-name>controller.GeneroController</servlet-name>
      <url-pattern>/genero</url-pattern>
    </servlet-mapping>


      <servlet>
      <servlet-name>controller.AlbumController</servlet-name>
      <servlet-class>controller.AlbumController</servlet-class>
    </servlet>
 
    <servlet-mapping>
      <servlet-name>controller.AlbumController</servlet-name>
      <url-pattern>/album</url-pattern>
    </servlet-mapping>

        <servlet>
      <servlet-name>controller.CancionController</servlet-name>
      <servlet-class>controller.CancionController</servlet-class>
    </servlet>
 
    <servlet-mapping>
      <servlet-name>controller.CancionController</servlet-name>
      <url-pattern>/cancion</url-pattern>
    </servlet-mapping>
</web-app>
